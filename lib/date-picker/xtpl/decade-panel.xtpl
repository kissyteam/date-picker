<div class="{{getBaseCssClasses ('header')}}">
    <a class="{{getBaseCssClasses ('prev-century-btn')}}"
       href="javascript:void(0)"
       role="button"
       title="{{previousCenturyLabel}}"
       hidefocus="on">
    </a>
    <div class="{{getBaseCssClasses ('century')}}">
                {{startYear}}-{{endYear}}
    </div>
    <a class="{{getBaseCssClasses ('next-century-btn')}}"
       href="javascript:void(0)"
       role="button"
       title="{{nextCenturyLabel}}"
       hidefocus="on">
    </a>
</div>
<div class="{{getBaseCssClasses ('body')}}">
    <table class="{{getBaseCssClasses ('table')}}" cellspacing="0" role="grid">
        <tbody class="{{getBaseCssClasses ('tbody')}}">
        {{{include ("./decades")}}}
        </tbody>
    </table>
</div>